{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Elastic.Serilog.Sinks" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Elasticsearch",
        "Args": {
          "bootstrapMethod": "Silent", // 引导方法：Silent（尝试安装模板但失败时不抛异常）
          "nodes": [ "http://elasticsearch:9200" ], // Elasticsearch 节点 URL（替换为您的实际地址）
          //"useSniffing": true, // 可选：启用节点嗅探
          //"apiKey": "<您的 API Key，如果需要>", // 可选：API Key 认证
          "username": "elastic", // 可选：Basic Auth
          "password": "yourpassword", // 可选：Basic Auth
          // 数据流和 ILM 配置（可选，根据文档）
          "ilmPolicy": "cocity-api-policy", // ILM 策略
          "dataStream": "cocity-api" // 数据流名称（默认 logs-dotnet-default）
          // 包含丰富信息（可选）
          // "includeHost": true,
          // "includeUser": true,
          // "includeProcess": true,
          // "includeActivity": true,
          // 过滤属性（可选）
          // "filterProperties": [ "prop1", "prop2" ], // 排除某些属性
          // 代理配置（可选，如果需要代理）
          // "proxy": "http://localhost:8200",
          // "proxyUsername": "x",
          // "proxyPassword": "y",
          // 专家设置（可选，仅在需要时使用）
          // "maxRetries": 3,
          // "maxConcurrency": 20,
          // "maxInflight": 100000,
          // "maxExportSize": 1000,
          // "maxLifeTime": "00:00:05",
          // "fullMode": "Wait"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ] // 可选：添加 enrichers
  },
  "AllowedHosts": "*",
  "AllowedOrigins": [
    "https://example1.com",
    "https://example2.com"
  ],
  "LocalhostToken": "your-local-access-token-here",
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=CoCity;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "Jwt": {
    "SecretKey": "your-super-secret-key-that-is-at-least-32-characters-long",
    "Issuer": "issuer",
    "Audience": "audience"
  }
}